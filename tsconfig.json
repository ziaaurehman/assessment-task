{
  "extends": "@react-native/typescript-config/tsconfig.json",
  "compilerOptions": {
    "module": "esnext",
    "baseUrl": "./src",
    "paths": {
      "@assets/*": ["assets/*"],
      "@components/*": ["components/*"],
      "@buttons/*": ["components/buttons/*"],
      "@text/*": ["components/text/*"],
      "@views/*": ["components/views/*"],
      "@models/*": ["models/*"],
      "@navigation/*": ["navigation/*"],
      "@screens/*": ["screens/*"],
      "@utils/*": ["utils/*"],
      "@icons/*": ["utils/icons/*"],
    }
  }
}